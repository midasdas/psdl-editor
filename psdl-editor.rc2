	END
	POPUP "&Tools"
	BEGIN
		MENUITEM "&Launch MM2\tF5"				ID_TOOLS_MM2
		MENUITEM "Open Con&taining Folder"		ID_FILE_OPENCONTAININGFOLDER
		MENUITEM SEPARATOR
		MENUITEM "Optimize &PSDL..."			ID_TOOLS_OPTIMIZE
		MENUITEM SEPARATOR
		MENUITEM "&Options..."					ID_TOOLS_OPTIONS
	END
	POPUP "&Help"
	BEGIN
		MENUITEM "&Contents\tF1"				ID_HELP_INDEX
		MENUITEM SEPARATOR
		MENUITEM "&About PSDL Editor..."		ID_APP_ABOUT
	END
END

