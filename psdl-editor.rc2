	BEGIN
		MENUITEM "&City Block\tCtrl+B"			ID_INSERT_CITYBLOCK
		MENUITEM "&Vertex"						ID_INSERT_VERTEX
		MENUITEM "&Texture"						ID_INSERT_TEXTURE					GRAYED
		MENUITEM SEPARATOR
